        "editorGroupHeader.tabsBackground" : "#111111",
        "editorGroup.border": "#222222",
        "editor.lineHighlightBackground": "#212121",
        "tab.activeBackground": "#222222",
        "tab.inactiveBackground": "#111111",
    },
    "go.useLanguageServer": true
